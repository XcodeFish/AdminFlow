# 应用配置
VITE_APP_TITLE=AdminFlow
VITE_APP_ENV=development

# API配置
VITE_API_BASE_URL=http://localhost:7080/api
VITE_API_TIMEOUT=30000

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# 本地开发配置
VITE_PORT=3000
VITE_OPEN_BROWSER=true
VITE_DROP_CONSOLE=false

# 构建配置
VITE_OUTPUT_DIR=dist
VITE_BASE_PUBLIC_PATH=/

# 代码生成器配置
VITE_CODE_GENERATOR_ENABLED=true
VITE_DEFAULT_TEMPLATE=standard

# UI主题配置
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_SWITCHING=true

# 安全配置
VITE_ENABLE_PERMISSION=true
VITE_ENABLE_AUTHENTICATION=true

# 缓存配置
VITE_PERSIST_ENABLED=true
VITE_PERSIST_STORAGE=localStorage
